                    <li><a href="/myprofile">Home</a></li>
                    <li><a href="auth/logout">Logout</a></li>
                </ul>
                <% if (user.vkontakte.token) { %>
                    <span><a href="/profile" class="navbar-link"><img class="myAvatar navbar-right" src='<%= user.vkontakte.avatar %>' /></a></span>
                    <p class="navbar-text navbar-right">Hello <a href="/profile" class="navbar-link"><%= user.vkontakte.name.firstName %></a></p>
                <% } else { 
                        if (user.facebook.token) { %>
                            <p class="navbar-text navbar-right">Hello <a href="/profile" class="navbar-link"><%= user.facebook.name.firstName %></a></p>
                    <% } else { 
                            if (user.google.token) { %>
                            
